{
  "name": "composite-scopes-all",
  "version": "1.1.1",
  "description": "Context-oriented programming beyond partial behavior",
  "main": "dist/composite-scopes-all.js",
  "jsnext:main": "src/composite-scopes-all.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "set NODE_ENV=production && webpack --define process.env.NODE_ENV='\"production\"'",
    "watch": "webpack -d --watch",
    "test": "karma start --single-run",
    "coverage": "karma start karma-coverage.conf.js",

    "old-test": "npm run test-node && npm run test-karma",
    "test-karma": "karma start --single-run",
    "karma": "karma",
    "test-node": "mocha tests --require tests/globalChai.js --require tests/registerBabel.js",
    "test-chrome-only": "karma start --single-run --browsers Chrome",
    "test-continuously": "karma start",
    "test-continuously-chrome-only": "karma start --browsers Chrome",
    "test-canary": "karma start --browsers ChromeCanary"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/composite-scopes/composite-scopes-all.git"
  },
  "keywords": [
    "COP",
